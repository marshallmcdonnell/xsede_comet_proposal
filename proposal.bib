@Article{Kluth2011,
  Title                    = {Nanoscale density fluctuations in swift heavy ion irradiated amorphous {SiO2}.},
  Author                   = {Kluth, P. and Pakarinen, O. H. and Djurabekova, F. and Giulian, R. and Ridgway, M. C. and Byrne, A. P. and Nordlund, K.},
