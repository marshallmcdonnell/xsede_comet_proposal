@Article{Krasheninnikov2010,
  Title                    = {Ion and electron irradiation-induced effects in nanostructured materials.},
  Author                   = {Krasheninnikov, A. V. and Nordlund, K.},
  Journal                  = {Journal of Applied Physics},
  Year                     = {2010},
  Number                   = {7},
  Pages                    = {071301 - 071301-70},
  Volume                   = {107},

  Abstract                 = {A common misconception is that the irradiation of solids with energetic electrons and ions has exclusively detrimental effects on the properties of target materials. In addition to the well-known cases of doping of bulk semiconductors and ion beam nitriding of steels, recent experiments show that irradiation can also have beneficial effects on nanostructured systems. Electron or ion beams may serve as tools to synthesize nanoclusters and nanowires, change their morphology in a controllable manner, and tailor their mechanical, electronic, and even magnetic properties. Harnessing irradiation as a tool for modifying material properties at the nanoscale requires having the full microscopic picture of defect production and annealing in nanotargets. In this article, we review recent progress in the understanding of effects of irradiation on various zero-dimensional and one-dimensional nanoscale systems, such as semiconductor and metal nanoclusters and nanowires, nanotubes, and fullerenes. W},
  ISSN                     = {00218979},
  Keywords                 = {Irradiation, Nitriding, Surface hardening, Nanostructured materials, Electron beams, Ion bombardment},
  Owner                    = {ntm},
  Timestamp                = {2017.01.13},
  Url                      = {http://proxy.lib.utk.edu:90/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=a9h&AN=49193246&scope=site}
}

